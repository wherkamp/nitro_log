{
  "default_loggers": [
    {
      "targets": [
        {
          "type": "console",
          "properties": {
            "format": "%module% %date_time{format=''}% %level%: %message%"
          }
        }
      ]
    }
  ],
  "loggers": [
    {
      "module": "normal",
      "targets": [
        {
          "type": "console",
          "properties": {
            "format": "%module% %level%: %message%"
          }
        },
        {
          "type": "file-logger",
          "properties": {
            "format": "%module% %level%: %message%",
            "file": "logs/%module%/%level%.log"
          }
        }
      ]
    }
  ]
}
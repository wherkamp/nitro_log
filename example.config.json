{
  "root_loggers": [
    {
      "levels": ["DEBUG","INFO", "WARN", "ERROR"],
      "format": "[{{module({})}}] {{level({})}}: {{message({})}}",
      "structure_dump": true,
      "targets": [
        {
          "type": "console",
          "properties": {
            "format": "[{{module({})}}] {{level({})}}: {{message({})}}"
          }
        }
      ]
    }
  ],
  "loggers": [
    {
      "module": "logger_tests",
      "format": "[{{module({})}}] {{level({})}}: {{message({})}}",
      "structure_dump": true,

      "targets": [
        {
          "type": "console"
        }
      ]
    }
  ]
}
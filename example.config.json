{
  "root_loggers": [
    {
      "format": {
        "format": "[{{module({})}}] {{level(LEVEL_SETTINGS)}}: {{message({})}}",
        "placeholders": {
          "LEVEL_SETTINGS": {
            "colored": false
          }
        }
      },
      "structure_dump": true,
      "targets": [
        {
          "type": "console"
        },
        {
          "type": "file_logger",
          "properties": {
            "file": "logs/{{module({})}}.log"
          }
        }
      ]
    }
  ]
}
{
  "default_loggers": [
    {
      "targets": [
        {
          "type": "console",
          "properties": {
            "format": "%datetime_{format='$Y-$m-$d $H:$M:$S'}% %level%: %message%"
          }
        }
      ]
    }
  ],
  "loggers": [
    {
      "module": "normal",
      "targets": [
        {
          "type": "console",
          "properties": {
            "format": "[%module%] %level_{color='true'}%: %message%"
          }
        },
        {
          "type": "file-logger",
          "properties": {
            "format": "%datetime_{format='$Y-$m-$d $H:$M:$S'}% %module% %level%: %message%",
            "file": "logs/%module%/%level%-%datetime_{format='$Y-$m-$d'}%.log"
          }
        }
      ]
    }
  ]
}
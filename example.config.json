{
  "root_loggers": [
    {
      "levels": ["DEBUG","INFO", "WARN", "ERROR"],
      "targets": [
        {
          "type": "console",
          "properties": {
            "format": "[{{module({})}}] {{level({})}}: {{message}}"
          }
        }
      ]
    }
  ],
  "loggers": [
    {
      "module": "logger_tests",
      "targets": [
        {
          "type": "console",
          "properties": {
            "format": "[{{module({})}}] {{level({})}}: {{message}}"
          }
        }
      ]
    }
  ]
}
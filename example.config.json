{
  "root_loggers": [
    {
      "format": {
        "format": "[{{chrono(DATE_TIME)}}]  [{{module({})}}] {{level(LEVEL_SETTINGS)}}: {{message({})}}",
        "placeholders": {
          "LEVEL_SETTINGS": {
            "colored": {}
          },
          "DATE_TIME": {
            "format": "%Y-%m-%d %H:%M:%S"
          }
        }
      },
      "structure_dump": true,
      "targets": [
        {
          "type": "console"
        },
        {
          "type": "file_logger",
          "properties": {
            "file": "logs/{{module({})}}.log"
          }
        }
      ]
    }
  ]
}